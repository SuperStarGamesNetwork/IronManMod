package perry.mods.IronManMod.Item;

import net.minecraft.client.renderer.texture.IconRegister;
import net.minecraft.item.EnumArmorMaterial;
import net.minecraft.item.ItemArmor;

public class ItemIronManBoots extends ItemArmor{

	public ItemIronManBoots(int par1, EnumArmorMaterial par2EnumArmorMaterial,
			int par3) {
		super(par1, par2EnumArmorMaterial, par3, par3);
		// TODO Auto-generated constructor stub
	}
	public void registerIcons(IconRegister par1IconRegister) {
		itemIcon = par1IconRegister.registerIcon("IronManMod:IronManBoots");
		
	}
}
